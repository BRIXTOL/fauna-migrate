{
  "name": "@brixtol/fauna-migrate",
  "version": "0.1.0",
  "description": "A migration helper utility for populating a serverless Fauna database.",
  "author": {
    "name": "Νίκος Σαβίδης ",
    "email": "n@brixtol.com",
    "url": "https://brixtoltextiles.com"
  },
  "license": "MIT",
  "main": "./package/index.js",
  "exports": {
    "require": "./package/index.js"
  },
  "types": "./package/@types/index.d.ts",
  "keywords": [
    "faunadb",
    "migration",
    "database",
    "fauna",
    "serverless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brixtol/fauna-migrate"
  },
  "bugs": {
    "url": "https://github.com/brixtol/fauna-migrate"
  },
  "engines": {
    "pnpm": ">=5"
  },
  "bin": {
    "fauna-migrate": "./bin/fauna-migrate"
  },
  "scripts": {
