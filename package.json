    "package": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "eslintConfig": {
    "extends": "@brixtol/eslint-config-typescript"
  },
  "devDependencies": {
    "@brixtol/eslint-config-typescript": "workspace:^1.0.0",
    "@brixtol/rollup-utils": "workspace:^0.2.0",
