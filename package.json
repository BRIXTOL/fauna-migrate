{
  "name": "@brixtol/fauna-migrate",
  "version": "0.1.3-beta.1",
  "description": "A migration helper utility for populating a serverless Fauna database.",
  "author": {
    "name": "Νίκος Σαβίδης ",
    "email": "n@brixtol.com",
    "url": "https://brixtoltextiles.com"
  },
  "license": "MIT",
  "main": "./package/index.mjs",
  "exports": {
    "require": "./package/index.cjs",
    "import": "./package/index.mjs"
  },
  "types": "./package/@types/index.d.ts",
  "keywords": [
    "faunadb",
    "migration",
    "database",
    "fauna",
    "serverless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brixtol/fauna-migrate"
  },
  "bugs": {
    "url": "https://github.com/brixtol/fauna-migrate"
  },
  "engines": {
    "pnpm": ">=6",
    "node": ">=14"
  },
  "bin": {
    "fauna-migrate": "./bin/index.js",
    "fm": "./bin/index.js"
  },
  "scripts": {
    "fm": "./bin/index.js",
    "fauna-migrate": "./bin/index.js",
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "package": "cd versions; pnpm pack ../ && cd ..",
    "test": "ava --watch --verbose"
  },
  "prettier": "@brixtol/prettier-config",
  "eslintConfig": {
    "ignorePatterns": "package",
    "extends": "@brixtol/eslint-config"
  },
  "ava": {
    "files": [
      "test/cli/lines.js"
    ],
    "concurrency": 5,
    "require": [
      "esm"
    ]
  },
  "esm": {
    "cjs": true
  },
  "devDependencies": {
    "@brixtol/eslint-config": "workspace:^1.0.1",
    "@brixtol/prettier-config": "workspace:^1.2.0",
    "@brixtol/rollup-config": "workspace:^1.4.0",
    "@brixtol/tsconfig": "workspace:^1.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "esm": "^3.2.25",
    "minimist": "^1.2.5",
    "ora": "^6.0.1"
  },
  "optionalDependencies": {
    "faunadb": "^4.4.1"
  }
}
